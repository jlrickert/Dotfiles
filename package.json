{
	"name": "@jlrickert/dotfiles",
	"private": true,
	"scripts": {
		"commit": "changeset",
		"changeset": "changeset --no-git-commit",
		"prettier": "prettier --write \"**/*.{md,yaml,json}\"",
		"release": "changeset publish",
		"version-packages": "changeset version && npm run prettier",
		"publish": "changeset version --no-git-tag"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.7",
		"prettier": "^3.3.3"
	}
}
